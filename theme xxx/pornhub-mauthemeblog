<script type='text/javascript'>
//<![CDATA[
$(function() {
    function e(e, t) {
        for (var a = 0; a < e[t].link.length; a++)
            if ("alternate" == e[t].link[a].rel) {
                var s = e[t].link[a].href;
                break
            } return s
    }

    function t(e, t, a) {
        return '<a href="' + a + '">' + e[t].title.$t + "</a>"
    }

    function a(e, t) {
        var a = e[t].title.$t,
            s = e[t].content.$t,
            i = $("<div>").html(s);
        if ("media$thumbnail" in e[t]) {
            var n = e[t].media$thumbnail.url,
                r = n.replace("s72-c", "w680");
            s.indexOf("youtube.com/embed") > -1 && (r = n.replace("/default.", "/hqdefault."))
        } else r = s.indexOf("<img") > -1 ? i.find("img:first").attr("src") : noThumbnail;
        return '<img class="post-thumb" alt="' + a + '" src="' + r + '"/>'
    }

    function s(e, t) {
        var a = e[t].content.$t,
            s = $("<div>").html(a).find('strike:contains("episode/")');
        if (0 < s.length) var i = s.text().split("/")[1];
        if (null != i) var n = '<span class="post_episode show">' + i + "</span>";
        else n = "";
        return [n]
    }

    function i(i, n, r, o) {
        if (n.match("mega-menu") || n.match("hot-posts") || n.match("featured") || n.match("grid-posts") || n.match("related")) {
            var l;
            l = "recent" == o ? "/feeds/posts/default?alt=json-in-script&max-results=" + r : "random" == o ? "/feeds/posts/default?max-results=" + r + "&start-index=" + (Math.floor(Math.random() * r) + 1) + "&alt=json-in-script" : "/feeds/posts/default/-/" + o + "?alt=json-in-script&max-results=" + r, $.ajax({
                url: l,
                type: "get",
                dataType: "jsonp",
                beforeSend: function() {
                    n.match("hot-posts") && i.html('<div class="hot-loader"/>').parent().addClass("show-hot")
                },
                success: function(r) {
                    if (n.match("mega-menu")) var l = '<ul class="mega-menu-inner">';
                    else n.match("hot-posts") ? l = '<ul class="hot-posts">' : n.match("featured") ? l = '<ul class="featured-posts">' : n.match("grid-posts") ? l = '<ul class="custom-widget">' : n.match("related") && (l = '<ul class="related-posts">');
                    var c = r.feed.entry;
                    if (null != c) {
                        for (var d = 0, h = c; d < h.length; d++) {
                            var m = e(h, d),
                                u = t(h, d, m),
                                p = a(h, d),
                                f = s(h, d),
                                g = "";
                            n.match("mega-menu") ? g += '<div class="mega-item item-' + d + '"><div class="mega-content"><a class="post-image-link" href="' + m + '">' + p + '</a><div class="post-info"><h2 class="post-title">' + u + "</h2></div></div></div>" : n.match("hot-posts") ? g += '<li class="hot-item item-' + d + '"><div class="hot-item-inner"><a class="post-image-link" href="' + m + '">' + p + "</a>" + f + '<div class="post-info"><h2 class="post-title">' + u + "</h2></div></div></li>" : n.match("featured") ? g += '<li class="feat-item item-' + d + '"><div class="featured-item-inner"><a class="post-image-link" href="' + m + '">' + p + "</a>" + f + '<div class="post-info"><h2 class="post-title">' + u + "</h2></div></div></li>" : n.match("grid-posts") ? g += '<li class="item-' + d + '"><a class="post-image-link" href="' + m + '">' + p + '</a><div class="post-info"><h2 class="post-title">' + u + "</h2></div></li>" : n.match("related") && (g += '<li class="related-item item-' + d + '"><div class="related-item-inner"><a class="post-image-link" href="' + m + '">' + p + '</a><div class="post-info"><h2 class="post-title">' + u + "</h2></div></div></li>"), l += g
                        }
                        l += "</ul>"
                    } else l = "<ul class=\"no-posts\"><b>Error:</b> No Posts Found <i class='far fa-frown'/></ul>";
                    if (n.match("mega-menu")) i.addClass("has-sub mega-menu").append(l), i.find("a:first").attr("href", function(e, t) {
                        return "recent" == o || "random" == o ? t.replace(t, "/search/?&max-results=" + postPerPage) : t.replace(t, "/search/label/" + o + "?&max-results=" + postPerPage)
                    });
                    else if (n.match("hot-posts")) i.html(l).parent().addClass("show-hot");
                    else if (n.match("featured")) {
                        if ("recent" == o || "random" == o) var $ = "/search";
                        else $ = "/search/label/" + o;
                        i.parent().find(".widget-title").addClass("title-wrap").append('<a class="show-more" href="' + $ + "?&max-results=" + postPerPage + '">' + messages.showMore + "</a>"), i.html(l).parent().addClass("show-featured")
                    } else i.html(l)
                }
            })
        }
    }
    $("#main-menu").each(function() {
        for (var e = $(this).find(".LinkList ul > li").children("a"), t = e.length, a = 0; a < t; a++) {
            var s = e.eq(a),
                i = s.text();
            if ("_" !== i.charAt(0) && "_" === e.eq(a + 1).text().charAt(0)) {
                var n = s.parent();
                n.append('<ul class="sub-menu m-sub"/>')
            }
            "_" === i.charAt(0) && (s.text(i.replace("_", "")), s.parent().appendTo(n.children(".sub-menu")))
        }
        for (a = 0; a < t; a++) {
            var r = e.eq(a),
                o = r.text();
            if ("_" !== o.charAt(0) && "_" === e.eq(a + 1).text().charAt(0)) {
                var l = r.parent();
                l.append('<ul class="sub-menu2 m-sub"/>')
            }
            "_" === o.charAt(0) && (r.text(o.replace("_", "")), r.parent().appendTo(l.children(".sub-menu2")))
        }
        $("#main-menu ul li ul").parent("li").addClass("has-sub"), $("#main-menu .widget").addClass("show-menu")
    }), $("#main-menu-nav").clone().appendTo(".mobile-menu"), $(".mobile-menu .has-sub").append('<div class="submenu-toggle"/>'), $(".mobile-menu ul > li a").each(function() {
        var e = $(this),
            t = e.attr("href").trim(),
            a = t.toLowerCase(),
            s = t.split("/")[0];
        a.match("mega-menu") && e.attr("href", "/search/label/" + s + "?&max-results=" + postPerPage)
    }), $(".mobile-menu-toggle").on("click", function() {
        $("body").toggleClass("nav-active")
    }), $(".mobile-menu ul li .submenu-toggle").on("click", function(e) {
        $(this).parent().hasClass("has-sub") && (e.preventDefault(), $(this).parent().hasClass("show") ? $(this).parent().removeClass("show").find("> .m-sub").slideToggle(170) : $(this).parent().addClass("show").children(".m-sub").slideToggle(170))
    }), $(".show-search").on("click", function() {
        $("#nav-search, .mobile-search-form").fadeIn(250).find("input").focus()
    }), $(".hide-search").on("click", function() {
        $("#nav-search, .mobile-search-form").fadeOut(250).find("input").blur()
    }), $(".Label a, a.b-label, a.post-tag").attr("href", function(e, t) {
        return t.replace(t, t + "?&max-results=" + postPerPage)
    }), $(".avatar-image-container img").attr("src", function(e, t) {
        return (t = t.replace("/s35-c/", "/s45-c/")).replace("//img1.blogblog.com/img/blank.gif", "//4.bp.blogspot.com/-uCjYgVFIh70/VuOLn-mL7PI/AAAAAAAADUs/Kcu9wJbv790hIo83rI_s7lLW3zkLY01EA/s55-r/avatar.png")
    }), $("#post-body").shortcode({
        ads: function() {
            if (null != this.options) switch (this.options.id) {
                case "ads1":
                    return '<div id="new-before-ad"/>';
                case "ads2":
                    return '<div id="new-after-ad"/>';
                default:
                    return ""
            }
        }
    }), $("#new-before-ad").each(function() {
        var e = $(this);
        e.length && $("#before-ad").appendTo(e)
    }), $("#new-after-ad").each(function() {
        var e = $(this);
        e.length && $("#after-ad").appendTo(e)
    }), $("#main-before-ad .widget").each(function() {
        var e = $(this);
        e.length && e.appendTo($("#before-ad"))
    }), $("#main-after-ad .widget").each(function() {
        var e = $(this);
        e.length && e.appendTo($("#after-ad"))
    }), $("#main-arel-ad .widget").each(function() {
        var e = $(this);
        e.length && e.appendTo($("#arel-ad"))
    }), $(".post-episode-info").each(function() {
        var e = $(this),
            t = e.data("id");
        $.ajax({
            url: "/feeds/posts/default/" + t + "?alt=json",
            type: "get",
            dataType: "jsonp",
            success: function(t) {
                var a = t.entry.content.$t,
